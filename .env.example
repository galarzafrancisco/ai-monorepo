# Backend Port (NestJS application)
PORT=3000

# Frontend Port (Vite development server)
VITE_PORT=5173

# Database Configuration
DATABASE_PATH=data/database.sqlite

# Node Environment
NODE_ENV=production

# Docker Host Port (used by docker-compose)
HOST_PORT=3000

# JWKS Configuration
# Time-to-live for JWKS keys in hours (default: 24 hours)
JWKS_KEY_TTL_HOURS=24
