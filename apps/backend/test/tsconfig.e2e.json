{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "resolvePackageJsonExports": false
  },
  "include": [
    "./**/*.ts",
    "../src/**/*.ts"
  ]
}
