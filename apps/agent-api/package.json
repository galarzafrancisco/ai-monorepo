{
  "name": "agent-api",
  "version": "1.0.0",
  "private": true,
  "description": "ADK-based agent API service with UV package management",
  "scripts": {
    "install": "uv sync",
    "dev": "uv run adk run src",
    "dev:api": "uv run adk api_server src",
    "dev:web": "uv run adk web src",
    "start": "uv run adk api_server src",
    "test": "uv run pytest"
  }
}
