        "tags": [
          "Client"
        ]
      },
      "delete": {
        "operationId": "ClientController_deleteClient",
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
            "description": "The unique identifier of the client",
            "schema": {
              "format": "uuid",
              "example": "a1b2c3d4-e5f6-7890-1234-567890abcdef",
